#include<iostream>
#include<cstring>
#include<cstdio>

using namespace std;

int main()
{
	char A[10000];
	int p,s;
	while(cin>>A){
	p = strlen(A);
	for(int i=0;i<p;i++)
    {
        if((A[i]=='A')||(A[i]=='B')||(A[i]=='C'))
        printf("2");
        else if((A[i]=='D')||(A[i]=='E')||(A[i]=='F'))
        printf("3");
        else if((A[i]=='G')||(A[i]=='H')||(A[i]=='I'))
        printf("4");
        else if((A[i]=='J')||(A[i]=='K')||(A[i]=='L'))
        printf("5");
        else if((A[i]=='M')||(A[i]=='N')||(A[i]=='O'))
        printf("6");
        else if((A[i]=='P')||(A[i]=='Q')||(A[i]=='R')||(A[i]=='S'))
        printf("7");
        else if((A[i]=='T')||(A[i]=='U')||(A[i]=='V'))
        printf("8");
        else if((A[i]=='W')||(A[i]=='X')||(A[i]=='Y')||(A[i]=='Z'))
        printf("9");
        else
        printf("%c",A[i]);
        }
        printf("\n");
        }
        return 0;
	}
